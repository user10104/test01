package ex01;

public class Year {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int year = 0;
		int age = 11;
		
		System.out.println(year);
		System.out.println(age);
		
		year = age + 1234; //변수 age값에 1234 더함 , 11+1234 
		age = age + 24; //qustn age저장된 값 1증가, 11+24
		
		System.out.println(year);
		System.out.println(age);
		
		//변수 선언 후에 초기화 필수

	}

}
