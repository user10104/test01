		long hex = 0xFFFF_FFFF_FFFF_FFFFL ;
		
		int octNum = 010; //8진수- 8
		int hexnum = 0x10; //16진수- 16
		int binNum = 0b10; //2진수- 2

		System.out.println("b=%D%N",b);
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
