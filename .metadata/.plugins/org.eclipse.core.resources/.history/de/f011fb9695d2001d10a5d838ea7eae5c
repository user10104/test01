package ex01;

public class Year {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int year = 0;
		int age = 11;
		
		System.out.println(year);
		System.out.println(age);
		
		year = age + 3000; //변수 age값에 2000 더함
		age = age + 1; //qustn age저장된 값 1증가
		
		System.out.println(year);
		System.out.println(age);
		

	}

}
